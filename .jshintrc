{
  // strict
  "bitwise":      false,
  "camelcase":    true,
  "curly":        true,
  "eqeqeq":       false,
  "es3":          false,
  "forin":        false,
  "freeze":       false,
  "immed":        true,
  "indent":       2,
  "latedef":      true,
  "newcap":       true,
  "noarg":        true,
  "noempty":      true,
  "nonew":        false,
  "plusplus":     false,
  "quotmark":     "double",
  "undef":        true,
  "strict":       true,
  "trailing":     true,
  "expr":         true,

  // relax
  "supernew":     true,
  "validthis":    true,

  // environment
  "browser":      true,

  // globals

  "globals": {
    "define":      false,
    "require":     false,
    "module":      false,
    "describe":    false,
    "beforeEach":  false,
    "it":          false,
    "expect":      false,
    "spyOn":       false,
    "setFixtures": false
  }
}
